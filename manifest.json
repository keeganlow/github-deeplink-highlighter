{
  "name": "Github Deeplink Highligher",
  "description": "Shows the comment, event, etc. for followed github links",
  "manifest_version": 2,
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["jquery.min.js", "highlight-anchor.js"]
    }
  ],
  "permissions": ["tabs"]
}
